        <api_name>x_0505_cc_queryb.isAfter</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns a filter query expression when a date field occurred after a date</description>
        <name>isAfter</name>
        <script><![CDATA[var isAfter = function(date) {
	if (_GlideSystemDates[date]) {
		return  '>javascript:gs.endOf' + date + '()';
	}
	else {
		return '>' + _dateGenerate(date);
	}
};]]></script>
