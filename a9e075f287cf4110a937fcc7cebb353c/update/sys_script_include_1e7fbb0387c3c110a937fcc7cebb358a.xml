        <api_name>x_0505_cc_queryb.isBefore</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns a filter query expression when a date field occurred before a date</description>
        <name>isBefore</name>
        <script><![CDATA[var isBefore = function(date) {
	if (_GlideSystemDates[date]) {
		return  '<javascript:gs.beginningOf' + date + '()';
	}
	else {
		return '<' + _dateGenerate(date);
	}
};]]></script>
